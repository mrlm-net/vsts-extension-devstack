{
    "name": "@mrlm.net/vsts-extension-devstack",
    "version": "0.1.6",
    "license": "Apache-2.0",
    "description": "Simple abstract node module to make development of VSTS extensions easy.",
    "type": "module",
    "files": [
        "dist"
    ],
    "bin": "./dist/index.js",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./devserver": {
            "import": "./dist/devserver.js",
            "require": "./dist/devserver.cjs"
        }
    },
    "devDependencies": {
        "@mrlm.net/vsts-extension-devstack": "0.1.4",
        "@types/node": "^22.5.5",
        "vite": "^5.4.5",
        "vite-plugin-dts": "^4.2.1"
    },
    "dependencies": {
        "copy-webpack-plugin": "^12.0.2",
        "webpack": "^5.94.0",
        "webpack-dev-server": "^5.1.0"
    }
}
